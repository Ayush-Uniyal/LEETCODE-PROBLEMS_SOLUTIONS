vector<int> nse(n,-1);
    stack<int> st;
    for(int i = n-1;i>=0;i--){
        while(!st.empty() && st.top()>=arr[i]){
         st.pop();
        }
        if(i<n){
            if(!st.empty()){
                nse[i] = st.top();
            }
        }
        st.push(arr[i]);
    }
    return nse;
}
